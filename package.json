{
    "name": "cxxlayout",
    "version": "0.1.0",
    "description": "C++ Record Layout Visualizer - Analyze and visualize C++ struct/class memory layouts",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "serve": "npx serve -s . -p 8080",
