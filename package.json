        "demo": "npm run build && npm run serve"
    },
    "keywords": ["c++", "memory-layout", "struct", "class", "visualization", "clang"],
    "author": "Iris Shi",
    "license": "MIT",
    "devDependencies": {
        "@types/emscripten": "^1.40.1",
        "typescript": "^5.0.0",
        "serve": "^14.0.0"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}